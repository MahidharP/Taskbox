<h3>Listing Projects
</h3>
<h4>Total Projects:
	<%= @projects.size %>
</h4>
<table border="10">
	<thead>
		<tr>
			<th>Name</th>
			<th>Client</th>
			<th>Start Date</th>
			<th>End Date</th>
		</tr>
	</thead>

	<% @projects.each do |project| %>
	<tr>
		<td><%= project.name %></td>
		<td><%= Client.find(project.client_id).name %></td>
		<td><%= project.start_date.strftime("%A, %d %B %y") %></td>
		<td><%= project.end_date.strftime("%A, %d %B %y") %></td>
	</tr>
	<%end %>
	<tbody></tbody>
